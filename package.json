    "react": "^16.8.5",
    "react-dom": "^16.8.5",
    "rimraf": "^2.6.3",
    "styled-components": "^4.1.3",
    "timespan": "^2.3.0"
  },
  "author": "Oh Kay",
  "homepage": "https://khtdr.com/react-zondicons",
  "license": "CC-BY-1.0",
  "repository": "https://github.com/khtdr/react-zondicons.git",
