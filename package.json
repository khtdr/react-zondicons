{
  "name": "react-zondicons",
  "version": "1.1.6",
  "description": "React Zondicon Component Collection",
  "main": "lib/index.js",
  "module": "es/index.js",
  "typings": "types/index.d.ts",
  "files": [
    "css",
    "es",
    "lib",
    "umd",
    "types"
  ],
  "scripts": {
    "zondicons": "node scripts/build.js",
